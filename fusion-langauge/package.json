{
  "name": "fusion-langauge",
  "displayName": "Fusion langauge",
  "description": "Fusion programming langauge ",
  "publisher": "brunoblaise",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/brunoblaise1/Fusion.git"
  },
  "icon": "/fusion.png",
  "contributes": {
    "languages": [
      {
        "id": "fusion",
        "aliases": [
          "Fusion",
          "fusion"
        ],
        "extensions": [
          ".fu"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "fusion",
        "scopeName": "source.fu",
        "path": "./syntaxes/fusion.tmLanguage.json"
      }
    ],
    "iconThemes": [
      {
        "id": "fusion",
        "label": "Fusion Icon",
        "path": "icons.json"
      }
    ]
  }
}
